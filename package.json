{
  "name": "material-ui-selectfield",
  "version": "1.0.14",
  "description": "material-ui-selectfield React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "gpublish": "nwb build-demo && gh-pages -d demo/dist"
  },
  "dependencies": {
    "material-ui-icons": "1.0.0-beta.15",
    "react-infinite": "^0.12.1",
    "react-virtualized": "^9.10.1",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "material-ui": ">=1.0.0-beta.13",
    "material-ui-icons": ">=1.0.0-beta.15",
    "react": ">=15.6.x",
    "react-dom": ">=15.6.x",
    "react-custom-scrollbars": ">=4.1.2",
    "react-infinite": ">=0.12.1",
    "react-virtualized": ">=9.10.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.0",
    "material-ui": "1.0.0-beta.13",
    "nwb": "0.18.x",
    "react": "15.6.2",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "15.6.2"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 3%",
              "ie 11",
              "safari 8"
            ]
          },
          "modules": "commonjs",
          "debug": false
        }
      ],
      "react"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard"
    ],
    "parser": "babel-eslint",
    "rules": {
      "camelcase": "off",
      "brace-style": "off",
      "no-trailing-spaces": "off",
      "no-multiple-empty-lines": "off",
      "no-unused-vars": "warn"
    }
  },
  "author": "RaphaÃ«l Morineau <raphael.morineau@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robksawyer/material-ui-superselectfield"
  },
  "homepage": "https://github.com/robksawyer/material-ui-superselectfield",
  "keywords": [
    "react-component",
    "material-ui",
    "material select"
  ]
}
